#!/bin/bash
#set -e
echo "#################################"
echo "    Updating ZSHrc to fastest    "
echo "#################################"
echo
echo "Step 1 - Cleaning up ZSHrc"
echo "#################################"
rm $HOME/.zshrc
sleep 3
echo
echo "Step 2 - Grabbing Latest ZSHrc"
echo "#################################"
cd $HOME/ && wget https://raw.githubusercontent.com/xerolinux/xero-fixes/main/conf/.zshrc
sleep 3
echo "#################################"
echo "  Updating ZSHrc Done! Exit Me   "
echo "#################################"
